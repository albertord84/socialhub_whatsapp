{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-websockets": "^1.3",
        "bugsnag/bugsnag-laravel": "^2.0",
        "coenjacobs/eloquent-composite-primary-keys": "^1.0",
        "doctrine/dbal": "~2.3",
        "doctrine/inflector": "1.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.4",
        "infyomlabs/adminlte-templates": "^5.7",
        "infyomlabs/laravel-generator": "^5.7",
        "kylekatarnls/laravel-carbon-2": "^1.0.0",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^3.0",
        "laravel/telescope": "~1.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.7",
        "nesbot/carbon": "2.0.0-beta.2 as 1.39.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "~3.0",
        "tymon/jwt-auth": "1.0.0-rc.3",
        "vindi/vindi-php": "^1.2"
    },
    "require-dev": {
        "albertord84/auth-tests": "1.0",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "composer/composer": "^1.9",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.9",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.4",
        "xethron/migrations-generator": "^2.0",
        "stavarengo/php-sigep": "dev-master",
        "stavarengo/php-sigep-fpdf": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": {
        "auth-tests": { 
            "type": "package",
            "package": {
                "name": "albertord84/auth-tests",  
                "version": "1.0",
                "source": {
                    "url": "https://github.com/albertord84/auth-tests", 
                    "type": "git", 
                    "reference": "origin/master"
                }
            }
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
